{
  "executionRoleArn": "arn:aws:iam::400899826069:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/web3hooks-prod",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "entryPoint": [],
      "portMappings": [
        {
          "hostPort": 4000,
          "protocol": "tcp",
          "containerPort": 4000
        }
      ],
      "environment": [
        {
          "name": "MONGO_DB_URI",
          "value": "arn:aws:ssm:us-east-1:400899826069:parameter/mongodb_prod_url"
        }
      ],
      "image": "400899826069.dkr.ecr.us-east-1.amazonaws.com/web3hooks",
      "essential": true,
      "name": "web3hooks-server-prod"
    }
  ],
  "memory": "512",
  "family": "web3hooks-server-prod",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256"
}